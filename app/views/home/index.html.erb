<% title "Welcome to Brno Ruby Meetup"%>

<% unless user_signed_in? %>
  <%= link_to "Sign in with Facebook", user_omniauth_authorize_path(:facebook) %> or 
  <%= link_to "sign in with google", user_omniauth_authorize_path(:open_id, :openid_url => "https://www.google.com/accounts/o8/id") %>
<% else %>
  <%= ap current_user %>
<% end %>
